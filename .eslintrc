{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "rules": {
    "quotes": 0,
    "comma-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "eol-last": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": "off",
    "react/prefer-stateless-function": "off"
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "jquery": true,
    "jasmine": true
  }
}
